{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "jsx": "react-jsx"
  },
  "strict": true,
  "ignore": [
    "className"
  ],
  "noUnusedLocals": true,
  "noUnusedParameters": true,
  "noFallthroughCasesInSwitch": true,
  "include": [
    "global.d.ts",
    "**/*.{ts,tsx,js,jsx}",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
