{
  "expo": {
    "scheme": "med-donation",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ],
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ]
    ],
    "name": "med-donation",
    "slug": "med-donation",
    "experiments": {
      "typedRoutes": true
    }
  }
}
